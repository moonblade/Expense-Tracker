{
  "templates": [
    {
      "sender": "noreply@phonepe.com",
      "senderTemplates": [
        {
          "subjectRegex": "Sent ₹(.*) to (.*)",
          "groups": ["amount", "payee"],
          "contentTemplates": [{
            "key": "transactionId",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. ID"
          }, {
            "key": "transactionStatus",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. Status"
          }, {
            "key": "debitedFrom",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Debited From"
          }]
        },{
          "subjectRegex": "Payment for (.*) of ₹(.*) is successful",
          "groups": ["payee", "amount"],
          "contentTemplates": [{
            "key": "transactionId",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. ID"
          }]
        }
      ]
    }, {
      "sender": "credit_cards@icicibank.com",
      "senderTemplates": [
        {
          "subjectRegex": "(.*)",
          "groups": ["subject"]
        }
      ]
    }
  ]

}
