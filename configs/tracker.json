{
  "templates": [
    {
      "sender": "noreply@phonepe.com",
      "senderTemplates": [
        {
          "subjectRegex": "Sent ₹(.*) to (.*)",
          "groups": ["amount", "payee"],
          "contentTemplates": [{
            "key": "transactionId",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. ID"
          }, {
            "key": "transactionStatus",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. Status"
          }, {
            "key": "account",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "debited from"
          }, {
            "key": "bankRefNo",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Bank Ref. No."
          }, {
            "key": "message",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Message"
          }]
        },{
          "subjectRegex": "Payment for (.*) of ₹(.*) is successful",
          "groups": ["payee", "amount"],
          "contentTemplates": [{
            "key": "transactionId",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. ID"
          }, {
            "key": "bankRefNo",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Bank Ref. No."
          }, {
            "key": "account",
            "type": "findByText",
            "tagName": "span",
            "findNext": ["span", "span"],
            "text": "Account"
          }, {
            "key": "transactionStatus",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Txn. Status"
          },{
            "key": "paymentFor",
            "type": "findByText",
            "tagName": "span",
            "findNext": ["span"],
            "text": "Payment For"
          },{
            "key": "category",
            "type": "findByText",
            "tagName": "td",
            "findNext": ["td", "td"],
            "text": "Category"
          }]
        }
      ]
    }, {
      "sender": "credit_cards@icicibank.com",
      "senderTemplates": [
        {
          "subjectRegex": "Transaction alert for your ICICI Bank Credit Card",
          "groups": [],
          "contentTemplates": [{
            "type": "regexInText",
            "regex": "Your (.*) has been used for a transaction of INR (.*) on (.*). Info: (.*). .*",
            "comment": "Keep date as transactionId",
            "groups": ["account", "amount", "transactionId", "payee"]
          }]
        }
      ]
    }, {
      "sender": "customercare@icicibank.com",
      "senderTemplates": [{
        "subjectRegex":  "Transaction alert for your ICICI Bank Internet Banking",
        "groups": [],
        "contentTemplates": [{
          "type": "regexInText",
          "regex": "You have made an online payment of INR (.*) towards (.*) from your Account (.*) on (.*). The Transaction ID is (.*).",
          "groups": ["amount", "payee", "account", "_date", "transactionId"]
        }]
      }]
    }, {
      "sender": "no-reply@paytmbank.com",
      "senderTemplates": [{
        "subjectRegex": "You paid Rs. (.*) to (.*)",
        "groups": ["amount", "payee"],
        "contentTemplates": [{
          "type": "findByText",
          "key": "transactionId",
          "tagName": "td",
          "text": "Transaction Id:"
        }, {
          "type": "static",
          "key": "account",
          "text": "paytm"
        }]
      }]
    }, {
      "sender": "no-reply@revolut.com",
      "senderTemplates": [{
        "subjectRegex": "You sent ₹(.*) to (.*)",
        "groups": ["amount", "payee"],
        "contentTemplates": [{
          "type": "regexInText",
          "regex": "You sent ₹(.*) to (.*). You can find the details below:",
          "groups": ["_date", "_date"]
        }, {
          "type": "static",
          "key": "account",
          "text": "revolut"
        }]
      }]
    }
  ]

}
